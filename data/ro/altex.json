        "paginate": {
          "next": ".Toolbar .Toolbar-pager a.Button--forSelect@href"
        },
        "item": "ul.Products li.Products-item",
        "product": {
          "url": "a.Product-photoTrigger@href",
          "title": "a.Product-photoTrigger@title",
          "image": "img.Product-photo@src",
