{
  "$schema": "../../schema-01.json",
  "version": "01",
  "country": "ro",
  "name": "Altex",
  "url": "https://www.altex.ro/",
  "currency": "RON",

  "lists": [
    {
      "language": "ro",
      "list": {
        "name": "Best sale",
        "productType": "smartphone",
